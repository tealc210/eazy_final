services:
    pgadmin-ib:
        stdin_open: true
        container_name: {{ INSTANCE_NAME}}
        ports:
            - {{ HTTP_PORT }}:80
            - {{ HTTPS_PORT }}:8443
        environment:
            - PGADMIN_DEFAULT_EMAIL={{ ADMIN_MAIL }}
            - PGADMIN_DEFAULT_PASSWORD={{ ADMIN_PASS }}
        image: rapidfort/pgadmin-ib:latest
